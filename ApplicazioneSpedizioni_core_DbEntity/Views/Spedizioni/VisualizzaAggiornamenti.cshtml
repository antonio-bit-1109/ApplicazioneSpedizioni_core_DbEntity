                @Html.DisplayNameFor(model => model.Spedizioni)
            </th> *@
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StatusSpedizione)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LuogoGiacenzaPacco)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descrizione)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataAggiornamento)
            </td>
