@model IEnumerable<ApplicazioneSpedizioni_core_DbEntity.Models.StatoSpedizione>

@{
    ViewData["Title"] = "VisualizzaAggiornamenti";
}

<h1>VisualizzaAggiornamenti</h1>

  <p>
    <a asp-action="Details" asp-route-id="@TempData["IdSpedizione"]"> Indietro</a>
</p> 

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StatusSpedizione)
